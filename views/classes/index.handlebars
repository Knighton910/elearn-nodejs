<h2>Classes</h2>
{{#each classes}}
	<div class="block">
		<h3>{{title}}</h3>
		<p>
			{{description}}
		</p>

		<p>Instructor: <strong>{{instructor}}</strong></p>
		<p>Teacher assistant: <strong>{{ta}}</strong></p>

		<a class="button" href="/classes/{{_id}}/details">Class Details</a>
	</div>
{{/each}}
